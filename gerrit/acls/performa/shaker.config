[access "refs/heads/*"]
abandon = group shaker-core
label-Code-Review = -2..+2 group shaker-core
label-Workflow = -1..+1 group shaker-core

[access "refs/tags/*"]
createSignedTag = group shaker-release
createTag = group shaker-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
