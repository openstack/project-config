[access]
	inheritFrom = openstack/meta-config

[access "refs/heads/*"]
	abandon = group puppet-manager-core
	abandon = group puppet-openstack-integration-core
	label-Code-Review = -2..+2 group puppet-manager-core
	label-Code-Review = -2..+2 group puppet-openstack-integration-core
	label-Workflow = -1..+1 group puppet-manager-core
	label-Workflow = -1..+1 group puppet-openstack-integration-core
