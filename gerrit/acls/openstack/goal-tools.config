[access]
	inheritFrom = openstack/meta-config

[access "refs/heads/*"]
	abandon = group goal-tools-core
	editHashtags = group goal-tools-core
	label-Code-Review = -2..+2 group goal-tools-core
	label-Workflow = -1..+1 group goal-tools-core

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
