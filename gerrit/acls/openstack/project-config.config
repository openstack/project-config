[access "refs/heads/*"]
	abandon = group project-config-core
	label-Code-Review = -2..+2 group project-config-core
	label-Workflow = -1..+1 group project-config-core

[access "refs/tags/*"]
	createSignedTag = group project-config-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
