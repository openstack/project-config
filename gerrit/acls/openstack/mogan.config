[access "refs/for/refs/*"]
pushMerge = group mogan-core

[access "refs/heads/*"]
abandon = group mogan-core
create = group mogan-release
label-Code-Review = -2..+2 group mogan-core
label-Workflow = -1..+1 group mogan-core

[access "refs/tags/*"]
pushSignedTag = group mogan-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
