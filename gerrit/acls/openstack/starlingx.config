[access]
inheritFrom = openstack/meta-config

[access "refs/for/refs/*"]
pushMerge = group starlingx-release

[access "refs/heads/*"]
abandon = group starlingx-core
create = group starlingx-release
label-Code-Review = -2..+2 group starlingx-core
label-Workflow = -1..+1 group starlingx-core

[access "refs/tags/*"]
pushSignedTag = group starlingx-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
