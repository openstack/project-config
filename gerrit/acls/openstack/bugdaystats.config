[access]
inheritFrom = openstack/meta-config

[access "refs/heads/*"]
abandon = group bugdaystats-core
label-Code-Review = -2..+2 group bugdaystats-core
label-Workflow = -1..+1 group bugdaystats-core

[access "refs/tags/*"]
createSignedTag = group bugdaystats-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
