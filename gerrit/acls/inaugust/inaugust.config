[access "refs/heads/*"]
abandon = group inaugust-core
label-Code-Review = -2..+2 group inaugust-core
label-Workflow = -1..+1 group inaugust-core

[access "refs/tags/*"]
createSignedTag = group inaugust-core
createTag = group inaugust-core

[receive]
requireChangeId = true

[submit]
mergeContent = true
