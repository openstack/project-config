[access "refs/heads/*"]
abandon = group ansible-role-borgmatic-core
label-Code-Review = -2..+2 group ansible-role-borgmatic-core
label-Workflow = -1..+1 group ansible-role-borgmatic-core

[access "refs/tags/*"]
createSignedTag = group ansible-role-borgmatic-release
createTag = group ansible-role-borgmatic-release

[receive]
requireChangeId = true
requireContributorAgreement = false

[submit]
mergeContent = true
