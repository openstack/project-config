[access "refs/heads/*"]
abandon = group puppet-planet-core
label-Code-Review = -2..+2 group puppet-planet-core
label-Workflow = -1..+1 group puppet-planet-core

[access "refs/tags/*"]
createSignedTag = group puppet-planet-release
createTag = group puppet-planet-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
