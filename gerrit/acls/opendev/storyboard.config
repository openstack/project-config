[access "refs/heads/*"]
abandon = group storyboard-core
label-Code-Review = -2..+2 group storyboard-core
label-Workflow = -1..+1 group storyboard-core

[access "refs/tags/*"]
createSignedTag = group storyboard-release
createTag = group storyboard-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
