[access "refs/heads/*"]
abandon = group bindep-core
label-Code-Review = -2..+2 group bindep-core
label-Workflow = -1..+1 group bindep-core

[access "refs/tags/*"]
createSignedTag = group bindep-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
