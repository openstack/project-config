[access "refs/heads/*"]
	abandon = group system-config-core
	label-Code-Review = -2..+2 group system-config-core
	label-Workflow = -1..+1 group system-config-core

[access "refs/tags/*"]
	createSignedTag = group system-config-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
