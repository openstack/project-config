[access "refs/heads/*"]
abandon = group meetbot-core
label-Code-Review = -2..+2 group meetbot-core
label-Workflow = -1..+1 group meetbot-core

[access "refs/tags/*"]
createSignedTag = group meetbot-release
createTag = group meetbot-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
