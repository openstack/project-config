[access "refs/heads/*"]
	abandon = group puppet-storyboard-core
	label-Code-Review = -2..+2 group puppet-storyboard-core
	label-Workflow = -1..+1 group puppet-storyboard-core

[access "refs/tags/*"]
	createSignedTag = group puppet-storyboard-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
