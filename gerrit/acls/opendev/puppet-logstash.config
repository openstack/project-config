[access "refs/heads/*"]
abandon = group puppet-logstash-core
label-Code-Review = -2..+2 group puppet-logstash-core
label-Workflow = -1..+1 group puppet-logstash-core

[access "refs/tags/*"]
createSignedTag = group puppet-logstash-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
