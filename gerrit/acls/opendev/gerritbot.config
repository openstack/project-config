[access "refs/heads/*"]
abandon = group gerritbot-core
label-Code-Review = -2..+2 group gerritbot-core
label-Workflow = -1..+1 group gerritbot-core

[access "refs/tags/*"]
createSignedTag = group gerritbot-release
createTag = group gerritbot-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
