[access "refs/heads/*"]
	abandon = group gozer-core
	label-Code-Review = -2..+2 group gozer-core
	label-Workflow = -1..+1 group gozer-core

[access "refs/tags/*"]
	createSignedTag = group gozer-core

[receive]
	requireChangeId = true
	requireContributorAgreement = true

[submit]
	mergeContent = true
