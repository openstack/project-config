[access "refs/heads/*"]
	abandon = group tobiko-core
	create = group tobiko-release
	label-Code-Review = -2..+2 group tobiko-core
	label-Workflow = -1..+1 group tobiko-core

[access "refs/tags/*"]
	createSignedTag = group tobiko-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
