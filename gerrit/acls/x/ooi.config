[access "refs/heads/*"]
abandon = group ooi-core
create = group ooi-release
label-Code-Review = -2..+2 group ooi-core
label-Workflow = -1..+1 group ooi-core

[access "refs/tags/*"]
createSignedTag = group ooi-release
createTag = group ooi-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
