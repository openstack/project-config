[access "refs/heads/*"]
	abandon = group valence-core
	create = group valence-release
	label-Code-Review = -2..+2 group valence-core
	label-Workflow = -1..+1 group valence-core

[access "refs/tags/*"]
	createSignedTag = group valence-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
