[access "refs/heads/*"]
	abandon = group broadview-lib-core
	label-Code-Review = -2..+2 group broadview-lib-core
	label-Workflow = -1..+1 group broadview-lib-core

[access "refs/tags/*"]
	createSignedTag = group broadview-lib-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
