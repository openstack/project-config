[access "refs/heads/*"]
	abandon = group delimiter-core
	create = group delimiter-release
	label-Code-Review = -2..+2 group delimiter-core
	label-Workflow = -1..+1 group delimiter-core

[access "refs/tags/*"]
	createSignedTag = group delimiter-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
