[access "refs/heads/*"]
abandon = group almanach-core
create = group almanach-release
label-Code-Review = -2..+2 group almanach-core
label-Workflow = -1..+1 group almanach-core

[access "refs/tags/*"]
createSignedTag = group almanach-release
createTag = group almanach-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
