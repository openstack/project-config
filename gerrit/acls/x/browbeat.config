[access "refs/heads/*"]
abandon = group browbeat-core
label-Code-Review = -2..+2 group browbeat-core
label-Workflow = -1..+1 group browbeat-core

[access "refs/tags/*"]
createSignedTag = group browbeat-release
createTag = group browbeat-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
