[access "refs/heads/*"]
abandon = group drbd-devstack-core
label-Code-Review = -2..+2 group drbd-devstack-core
label-Workflow = -1..+1 group drbd-devstack-core

[access "refs/tags/*"]
createSignedTag = group drbd-devstack-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
