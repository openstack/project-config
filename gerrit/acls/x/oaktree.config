[access "refs/heads/*"]
abandon = group oaktree-core
label-Code-Review = -2..+2 group oaktree-core
label-Workflow = -1..+1 group oaktree-core

[access "refs/tags/*"]
createSignedTag = group oaktree-release
createTag = group oaktree-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
