[access "refs/heads/*"]
abandon = group ansible-role-jenkins-plugins-core
label-Code-Review = -2..+2 group ansible-role-jenkins-plugins-core
label-Workflow = -1..+1 group ansible-role-jenkins-plugins-core

[access "refs/tags/*"]
createSignedTag = group ansible-role-jenkins-plugins-release
createTag = group ansible-role-jenkins-plugins-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
