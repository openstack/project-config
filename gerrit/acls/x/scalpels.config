[access "refs/heads/*"]
	abandon = group scalpels-core
	label-Code-Review = -2..+2 group scalpels-core
	label-Workflow = -1..+1 group scalpels-core

[access "refs/tags/*"]
	createSignedTag = group scalpels-release

[receive]
	requireChangeId = true
	requireContributorAgreement = true

[submit]
	mergeContent = true
