[access "refs/heads/*"]
	abandon = group openstack-spaceport-core
	create = group openstack-spaceport-release
	label-Code-Review = -2..+2 group openstack-spaceport-core
	label-Workflow = -1..+1 group openstack-spaceport-core

[access "refs/tags/*"]
	createSignedTag = group openstack-spaceport-release

[receive]
	requireChangeId = true
	requireContributorAgreement = true

[submit]
	mergeContent = true
