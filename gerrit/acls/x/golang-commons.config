[access "refs/heads/*"]
	abandon = group golang-commons-core
	label-Code-Review = -2..+2 group golang-commons-core
	label-Workflow = -1..+1 group golang-commons-core

[access "refs/tags/*"]
	createSignedTag = group golang-commons-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
