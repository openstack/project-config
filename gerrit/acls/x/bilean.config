[access "refs/heads/*"]
	abandon = group bilean-core
	label-Code-Review = -2..+2 group bilean-core
	label-Workflow = -1..+1 group bilean-core

[access "refs/tags/*"]
	createSignedTag = group bilean-release

[receive]
	requireChangeId = true
	requireContributorAgreement = true

[submit]
	mergeContent = true
