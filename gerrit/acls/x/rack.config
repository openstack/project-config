[access "refs/heads/*"]
abandon = group rack-core
create = group rack-core
label-Code-Review = -2..+2 group rack-core
label-Workflow = -1..+1 group rack-core

[access "refs/tags/*"]
createSignedTag = group rack-release
createTag = group rack-release

[receive]
requireChangeId = true

[submit]
mergeContent = true
