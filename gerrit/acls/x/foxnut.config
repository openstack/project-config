[access "refs/heads/*"]
	abandon = group foxnut-core
	label-Code-Review = -2..+2 group foxnut-core
	label-Workflow = -1..+1 group foxnut-core

[access "refs/tags/*"]
	createSignedTag = group foxnut-release

[receive]
	requireChangeId = true
	requireContributorAgreement = true

[submit]
	mergeContent = true
