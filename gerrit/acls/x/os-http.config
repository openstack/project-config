[access "refs/heads/*"]
abandon = group os-http-core
label-Code-Review = -2..+2 group os-http-core
label-Workflow = -1..+1 group os-http-core

[access "refs/tags/*"]
createSignedTag = group os-http-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
