[access "refs/heads/*"]
abandon = group ospurge-core
create = group ospurge-core
label-Code-Review = -2..+2 group ospurge-core
label-Workflow = -1..+1 group ospurge-core

[access "refs/tags/*"]
createSignedTag = group ospurge-release
createTag = group ospurge-release

[receive]
requireChangeId = true
requireContributorAgreement = false

[submit]
mergeContent = true
