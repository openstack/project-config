[access "refs/heads/*"]
	abandon = group vahana-core
	create = group vahana-release
	label-Code-Review = -2..+2 group vahana-core
	label-Workflow = -1..+1 group vahana-core

[access "refs/tags/*"]
	createSignedTag = group vahana-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
