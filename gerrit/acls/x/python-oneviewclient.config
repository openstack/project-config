[access "refs/heads/*"]
	abandon = group python-oneviewclient-core
	label-Code-Review = -2..+2 group python-oneviewclient-core
	label-Workflow = -1..+1 group python-oneviewclient-core

[access "refs/tags/*"]
	createSignedTag = group python-oneviewclient-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
