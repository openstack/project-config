[access "refs/heads/*"]
	abandon = group coupler-core
	create = group coupler-release
	label-Code-Review = -2..+2 group coupler-core
	label-Workflow = -1..+1 group coupler-core

[access "refs/tags/*"]
	createSignedTag = group coupler-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
