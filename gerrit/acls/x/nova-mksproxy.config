[access "refs/heads/*"]
	abandon = group nova-mksproxy-core
	label-Code-Review = -2..+2 group nova-mksproxy-core
	label-Workflow = -1..+1 group nova-mksproxy-core

[access "refs/tags/*"]
	createSignedTag = group nova-mksproxy-release

[receive]
	requireChangeId = true

[submit]
	mergeContent = true
