[access "refs/heads/*"]
	abandon = group merlin-core
	label-Code-Review = -2..+2 group merlin-core
	label-Workflow = -1..+1 group merlin-core

[access "refs/tags/*"]
	createSignedTag = group merlin-release

[receive]
	requireChangeId = true
	requireContributorAgreement = true

[submit]
	mergeContent = true
